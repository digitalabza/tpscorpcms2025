{
  "name": "acromedia/drupal-orange-project",
  "description": "Provides a template to create a new Drupal 9 project via Composer.",
  "license": "GPL-2.0-or-later",
  "type": "project",
  "require": {
    "ext-curl": "*",
    "bgrins/spectrum": "master",
    "centarro/centarro_claro": "1.x-dev",
    "ckeditor/ckeditor": "4.11.4",
    "ckeditor/colorbutton": "4.7.3",
    "ckeditor/font": "4.7.3",
    "ckeditor/panelbutton": "4.7.3",
    "composer/installers": "^1.9",
    "cweagans/composer-patches": "^1.7",
    "dimsemenov/magnific-popup": "master",
    "drupal/address": "^2.0",
    "drupal/adminimal_admin_toolbar": "^1.11",
    "drupal/asset_injector": "^2.17",
    "drupal/backup_migrate": "^5.0",
    "drupal/best_selling_products": "^3.12",
    "drupal/commerce": "2.38",
    "drupal/commerce_agree_terms": "^2.0",
    "drupal/commerce_checkout_order_fields": "^1.2",
    "drupal/commerce_feeds": "^1.0@alpha",
    "drupal/commerce_product_limits": "^1.0",
    "drupal/commerce_stock": "^1.1",
    "drupal/console": "^1.9",
    "drupal/core": "^9",
    "drupal/core-composer-scaffold": "9.5.11",
    "drupal/core-project-message": "9.5.11",
    "drupal/core-recommended": "9.5.11",
    "drupal/ctools": "3.14",
    "drupal/entity_embed": "1.5",
    "drupal/facets": "^1.8",
    "drupal/facets_pretty_paths": "1.4",
    "drupal/facets_range_dropdowns": "^2.0",
    "drupal/feeds": "^3.0@beta",
    "drupal/feeds_tamper": "^2.0@beta",
    "drupal/gin": "3.0-rc10",
    "drupal/gin_login": "^2.0",
    "drupal/gin_toolbar": "1.0-rc5",
    "drupal/jquery_ui": "1.6",
    "drupal/login_destination": "^2.0@beta",
    "drupal/login_history": "^1.1",
    "drupal/menu_block": "1.10",
    "drupal/orange_ecom_profile": "^2.0@alpha",
    "drupal/orange_profile": "2.0.0-alpha1",
    "drupal/pathauto": "1.11",
    "drupal/redis": "^1.7",
    "drupal/shield": "^1.7",
    "drupal/symfony_mailer": "^1.3",
    "drupal/tamper": "^1.0@alpha",
    "drupal/ultimate_cron": "^2.0@alpha",
    "drupal/user_redirect": "^2.0",
    "drupal/vbo_export": "^4.1",
    "drupal/views_bulk_operations": "^4.2",
    "drupal/webform": "6.2.2",
    "drush/drush": "^10",
    "recurser/jquery-simple-color": "master",
    "simeydotme/jquery-ui-slider-pips": "1.11.3",
    "vlucas/phpdotenv": "^5.6",
    "webflo/drupal-finder": "^1.3",
    "webmozart/path-util": "^2.3"
  },
  "require-dev": {
    "drupal/core-dev": "^9",
    "ergebnis/composer-normalize": "^2.43",
    "mglaman/phpstan-drupal": "^1.2",
    "phpspec/prophecy-phpunit": "^2.2",
    "phpstan/phpstan-deprecation-rules": "^1.2"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "vcs",
      "url": "https://git.drupalcode.org/project/orange_profile"
    },
    {
      "type": "vcs",
      "url": "https://git.drupalcode.org/project/orange_ecom_profile"
    },
    {
      "type": "package",
      "package": {
        "name": "ckeditor/colorbutton",
        "type": "drupal-library",
        "version": "4.7.3",
        "dist": {
          "type": "zip",
          "url": "https://download.ckeditor.com/colorbutton/releases/colorbutton_4.7.3.zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "ckeditor/font",
        "type": "drupal-library",
        "version": "4.7.3",
        "dist": {
          "type": "zip",
          "url": "https://download.ckeditor.com/font/releases/font_4.7.3.zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "ckeditor/panelbutton",
        "type": "drupal-library",
        "version": "4.7.3",
        "dist": {
          "type": "zip",
          "url": "https://download.ckeditor.com/panelbutton/releases/panelbutton_4.7.3.zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "ckeditor/ckeditor",
        "type": "drupal-library",
        "version": "4.11.4",
        "dist": {
          "type": "zip",
          "url": "https://github.com/ckeditor/ckeditor-dev/archive/4.11.4.zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "bgrins/spectrum",
        "type": "drupal-library",
        "version": "master",
        "source": {
          "type": "git",
          "url": "https://github.com/bgrins/spectrum",
          "reference": "origin/master"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "recurser/jquery-simple-color",
        "type": "drupal-library",
        "version": "master",
        "source": {
          "type": "git",
          "url": "https://github.com/recurser/jquery-simple-color",
          "reference": "origin/master"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "dimsemenov/magnific-popup",
        "type": "drupal-library",
        "version": "master",
        "source": {
          "type": "git",
          "url": "https://github.com/dimsemenov/Magnific-Popup",
          "reference": "origin/master"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "simeydotme/jquery-ui-slider-pips",
        "type": "drupal-library",
        "version": "v1.11.3",
        "source": {
          "type": "git",
          "url": "https://github.com/simeydotme/jQuery-ui-Slider-Pips",
          "reference": "v1.11.3"
        }
      }
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ],
    "files": [
      "load.environment.php"
    ]
  },
  "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "drupal/console-extend-plugin": true,
      "drupal/core-composer-scaffold": true,
      "drupal/core-project-message": true,
      "ergebnis/composer-normalize": true
    },
    "sort-packages": true
  },
  "extra": {
    "composer-exit-on-patch-failure": true,
    "drupal-scaffold": {
      "locations": {
        "web-root": "web/"
      }
    },
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
      ],
      "web/libraries/{$name}": [
        "type:drupal-library"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/Commands/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "web/modules/custom/{$name}": [
        "type:drupal-custom-module"
      ],
      "web/profiles/custom/{$name}": [
        "type:drupal-custom-profile"
      ],
      "web/themes/custom/{$name}": [
        "type:drupal-custom-theme"
      ]
    },
    "patchLevel": {
      "drupal/core": "-p2"
    },
    "patches": {
      "drupal/color_field": {
        "No functionality of the Spectrum widget on Drupal 8.6.x with latest stable or dev": "https://www.drupal.org/files/issues/2018-11-07/no_functionality_of_almost_all_widgets-3002836-7.patch"
      },
      "drupal/commerce_agree_terms": {
        "Fix checkout pane breakage": "https://www.drupal.org/files/issues/2023-08-07/3350343-4.patch"
      },
      "drupal/commerce_google_tag_manager": {
        "Use product variation SKU if available instead of product ID": "https://www.drupal.org/files/issues/2020-03-27/use-product-variation-sku-if-available-3066949-7-alpha3.patch"
      },
      "drupal/core": {
        "drupalimage CKEditor plugin should not require data-entity-uuid and data-entity-type when image upload is disabled": "https://www.drupal.org/files/issues/2018-09-13/drupalimage_ckeditor-2771837-34.patch",
        "Fix deprecated code in Drupal core syslog for PHP8": "https://www.drupal.org/files/issues/2023-01-14/3333215-11.patch"
      },
      "drupal/facets": {
        "Fix decimal in facets range slider": "https://www.drupal.org/files/issues/2023-07-14/do3317545-5.patch",
        "Fix JS in facets range slider": "https://www.drupal.org/files/issues/2021-01-08/facets-widget-range-slider.patch"
      },
      "drupal/google_analytics": {
        "Fix depracation of code": "https://www.drupal.org/files/issues/2022-02-02/deprecated-function-explode-3260466-2.patch"
      },
      "drupal/schema_metatag": {
        "Fix breadcrumb issue when filtering facets": "https://www.drupal.org/files/issues/2020-10-30/schema_metatag-fatal_error_in_SchemaItemListElementBreadcrumbBase.php-1.x-3107983-8.patch"
      }
    }
  }
}
